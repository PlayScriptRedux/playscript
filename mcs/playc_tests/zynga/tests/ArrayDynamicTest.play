package
{
	public class ArrayDynamicTest
	{
		public static function Main():int
		{
	   		var a:Array = [];
        	a[0] = 10;
        	a[1] = 11;
        	a[2] = 12;
        	a["key_a0"] = "a0";
        	a["key_a1"] = "a1";
        	a["key_a2"] = "a2";
			var o:Object;
			var strForEach:String = "";
        	for each(o in a)
        	{
        		strForEach += o;
        	}
        	if(strForEach != "101112a0a1a2")
        		throw new Error("Array - for each is wrong");
        	
        	var strForIn:String = "";  	
        	for ( o in a)
        	{
    	    	strForIn += o;
        	}
        	if(strForIn != "012key_a0key_a1key_a2")
        		throw new Error("Array - for in is wrong");
			
			return 0;
		}
	}
}

