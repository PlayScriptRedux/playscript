// Compiler options: -newdynamic+ -r:./play/Assert.dll
package
{
    import com.adobe.test.Assert;

	public dynamic class test
	{
		public static function Main():int
		{
			var res:int;
			res = Local ();
			if (res != 0)
				return res + 10;
				
			res = Imported ();
			if (res != 0)
				return res + 20;
				
			return 0;
		}
		
		static function Local ():int
		{
			var m:test = new test ();
			m["Prop"] = 500;
            m.Prop = 400;
            trace(m.Prop);
            Assert.expectEq( "m.Prop == 500", true, m.Prop == 500 );
            Assert.expectEq( "m['Prop'] == 500", true, m["Prop"] == 500 );
            m.Prop = 100;
            Assert.expectEq( "m['Prop'] == 100", true, m["Prop"] == 100 );
			m["Prop"] = 300;
            Assert.expectEq( "m.Prop != 300", true, m.Prop == 300 );
			return 0;
		}
		
		static function Imported ():int
		{
			var m:Array = new Array ();
			m["Prop"] = 500;
			if (m.Prop != 500)
				return 1;
				
			if (m["Prop"] != 500)
				return 2;

			if (m.Prop != 500)
				return 3;

			m.Prop = 300;
			if (m.Prop != 300)
				return 4;
				
			return Assert.errcount;
		}
	}
}
